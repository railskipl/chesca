<h1>Listing company_pages</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
  </tr>

<% @company_pages.each do |company_page| %>
  <tr>
    <td><%=h company_page.title %></td>
    <td><%=h company_page.content %></td>
    <td><%= link_to 'Edit', edit_admin_company_page_path(company_page) %></td>
    <td><%= link_to 'Delete', admin_company_page_path(company_page), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to 'New company_page', new_admin_company_page_path %>
